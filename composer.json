{
    "name": "evandersonluks/carpenter",
    "description": "An entity generator for evandersonluks applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Evanderson Lucas",
            "email": "evanlucas27@gmail.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/config": "^8.0 || ^9.0",
        "illuminate/support": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Evandersonluks\\Carpenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Evandersonluks\\Carpenter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Evandersonluks\\Carpenter\\CarpenterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
